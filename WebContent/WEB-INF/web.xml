<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SigninSystem</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.group.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UserSystem</servlet-name>
		<servlet-class>com.group.servlet.UserSystemServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Forget</servlet-name>
		<url-pattern>/Forget</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Forget</servlet-name>
		<servlet-class>com.group.servlet.ForgetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Modify</servlet-name>
		<url-pattern>/Modify</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Modify</servlet-name>
		<servlet-class>com.group.servlet.ModifyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserSystem</servlet-name>
		<url-pattern>/UserSystem</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>GetFilter</filter-name>
		<filter-class>com.group.servlet.GetFilter</filter-class>
		<init-param>
			<param-name>redirectPath</param-name>
			<param-value>/view/jsp/Login.jsp</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>GetFilter</filter-name>
		<url-pattern>/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
	</filter-mapping>
	<servlet>
    <description></description>
    <display-name>LeaveEditServlet</display-name>
    <servlet-name>LeaveEditServlet</servlet-name>
    <servlet-class>com.group.servlet.LeaveEditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LeaveEditServlet</servlet-name>
    <url-pattern>/LeaveEditServlet</url-pattern>
  </servlet-mapping>
  <jsp-config>
  	<taglib>
    	<taglib-uri>MyFirstTag</taglib-uri>
        <taglib-location>/WEB-INF/lib/MyTag.tld</taglib-location>
    </taglib>
  </jsp-config>
</web-app>